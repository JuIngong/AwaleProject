<Window x:Class="AwaleProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AwaleProject"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Awale" Height="400" Width="600">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
            <GradientStop Color="White" Offset="0.502"/>
            <GradientStop Color="#FFD32929" Offset="0.88"/>
            <GradientStop Color="#FFC11616" Offset="0.112"/>
        </LinearGradientBrush>
    </Window.Background>
    <Frame x:Name="ContentArea" NavigationUIVisibility="Hidden"/>
</Window>
